{
  "version": 2,
  "name": "forum-shaman2016",
  "builds": [
    {
      "src": "/api/session.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "headers": {
        "content-type": "text/html"
      },
      "dest": "/404.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
