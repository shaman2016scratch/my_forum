{
  "version": 2,
  "name": "forum-shaman2016",
  "builds": [
    {
      "src": "/api/session.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/([^.]+)\\.(html|css|js)$",
      "dest": "/$1.$2"
    },
    {
      "src": "/database/(.*)",
      "dest": "/database/$1"
    }
  ],
  "errors": {
    "404": {
      "source": "/(.*)",
      "destination": "/404.html",
      "headers": {
        "content-type": "text/html"
      }
    }
  }
}
